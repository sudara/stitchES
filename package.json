{
  "name": "stitches6",
  "version": "1.0.0",
  "description": "Stitching Together MP3 Playback in HTML5 Audio with ES6",
  "main": "index.js",
  "scripts": {
    "start": "http-server -s c-1 -p3000 . & ",
    "test": "npm start && nightwatch -c nightwatch-local.conf.json; npm stop",
    "stop": "pkill -f http-server",
    "localsauce": "sc -u ${SAUCELABS_USER} -k ${SAUCELABS_KEY} -i stitches-local -x https://eu-central-1.saucelabs.com/rest/v1 &",
    "nightwatch": "npm start && ./node_modules/.bin/nightwatch -c nightwatch-sauce-local.conf.js -e chrome; npm stop"
  },
  "repository": "git+https://github.com/sudara/stitches6.git",
  "keywords": [
    "mp3",
    "html5",
    "audio",
    "playback",
    "es6"
  ],
  "author": "Sudara",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sudara/stitches6/issues"
  },
  "homepage": "https://github.com/sudara/stitches6#readme",
  "private": false,
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "browserstack-local": "^1.4.2",
    "chromedriver": "77.0.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "http-server": "^0.11.1",
    "nightwatch": "^1.2.4",
    "prettier": "1.18.2",
    "saucelabs": "^3.0.1"
  },
  "prettier": {
    "semi": false
  }
}
